#include <stdio.h>
#include <stdlib.h>



int visited[MAX];


int queue[MAX], front = -1, rear = -1;

void enqueue(int v) {
    if (rear == MAX - 1) return;
    if (front == -1) front = 0;
    queue[++rear] = v;
}

int dequeue() {
    if (front == -1 || front > rear) return -1;
    return queue[front++];
}


void BFS(int adj[MAX][MAX], int n, int start) {
    int i, node;

    for (i = 0; i < n; i++)
        visited[i] = 0;

    enqueue(start);
    visited[start] = 1;

    printf("BFS Traversal: ");

    while ((node = dequeue()) != -1) {
        printf("%d ", node);

        for (i = 0; i < n; i++) {
            if (adj[node][i] == 1 && visited[i] == 0) {
                enqueue(i);
                visited[i] = 1;
            }
        }
    }
    printf("\n");
}


void DFS(int adj[MAX][MAX], int n, int start) {
    int i;
    visited[start] = 1;
    printf("%d ", start);

    for (i = 0; i < n; i++) {
        if (adj[start][i] == 1 && visited[i] == 0)
            DFS(adj, n, i);
    }
}

int main() {
    int n, adj[MAX][MAX], i, j, start;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (i = 0; i < n; i++)
        for (j = 0; j < n; j++)
            scanf("%d", &adj[i][j]);

    printf("Enter starting node: ");
    scanf("%d", &start);

   
    BFS(adj, n, start);

    
    for (i = 0; i < n; i++)
        visited[i] = 0;

    printf("DFS Traversal: ");
    DFS(adj, n, start);

    printf("\n");

    return 0;
}
